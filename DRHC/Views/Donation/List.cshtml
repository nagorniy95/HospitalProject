

@{
    ViewData["Title"] = "List";
}

<h2>Donor List</h2>

@foreach (var Donation in Model)
{
    <div> @Donation.DonationId</div>
    <div> @Donation.Title</div>
    <div> @Donation.FirstName</div>
    <div> @Donation.LastName</div>
    <div> @Donation.Address</div>
    <div> @Donation.City</div>
    <div> @Donation.Province</div>
    <div> @Donation.PostalCode</div>
    <div> @Donation.Email</div>
    <div> @Donation.Phone</div>
    <div> @Donation.Amount</div>
    






}

