@model IEnumerable<DRHC.Models.ERWaitTime>
            
@{
    ViewData["Title"] = "Show";
}

<h2>Show</h2>

<p>
    <a asp-action="New">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.WaitTimeCat)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Description)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.WaitTimeCat)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ERWaitTimeId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ERWaitTimeId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ERWaitTimeId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
